<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Hariom Milk</title>
    <link
    href="https://cdn.jsdelivr.net/npm/remixicon@4.5.0/fonts/remixicon.css"
    rel="stylesheet"
/>
    <link rel="stylesheet" href="/stylesheets/adminUserView.css">
    <link rel="manifest" href="/manifest.json">
<meta name="theme-color" content="#317EFB">
</head>
<body> 
    <main>
        <div class="header" style="font-size: 1.5rem;">
            <%=new Date().toLocaleDateString()%>
        </div>
        <select name="" id="">
            <option value="nagda">Nagda</option>
            <option value="palnaghar">Palnaghar</option>
            <option value="balgad">Balgad</option>
        </select>
       <div class="box">
        
        <%seller.forEach(function(val){%>
            <div class="user" id="<%=val._id%>">
                <div class="image">
                    <img src="<%=val.profilePicture%>" alt="404">
                </div>
                <div class="detail">
                    <div class="name"><%=val.username%></div>
                    <div class="address"><%=val.address%></div>                  
                </div>
                <i class="ri-arrow-right-s-line"></i>
            </div>
        <%})%>
       </div>
        <div class="fakeFooter"></div>
        <div class="footer">
            <div class="footerBox home">
                <img src="https://cdn-icons-png.flaticon.com/128/11744/11744074.png" alt="">
                <p>Products</p>
            </div>
            <div class="footerBox seller">
                <img src="https://cdn-icons-png.flaticon.com/128/4616/4616095.png" alt="">
                <p>User Details</p>
            </div>
            <div class="footerBox center cart">
                <img src="https://cdn-icons-png.flaticon.com/128/9752/9752284.png" alt="">
                <p>Orders</p>

            </div>
            <div class="footerBox milk">
                <img src="https://cdn-icons-png.flaticon.com/128/9058/9058574.png" alt="">
                <p>Milk Details</p>
            </div>
            <div class="footerBox more">
                <img src="https://cdn-icons-png.flaticon.com/128/5996/5996260.png" alt="">
                <p>More</p>
            </div>
        </div>
    </main>
    <script src="/javaScripts/adminNav.js"></script>
    <script src="/javaScripts/adminMilkUpload.js"></script>
    
</body>
</html>